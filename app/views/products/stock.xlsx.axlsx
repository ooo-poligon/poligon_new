wb = xlsx_package.workbook
wb.add_worksheet(name: "Продукты") do |sheet|
  sheet.add_row %w(Артикул Название Описание Производитель Stock RemoteStock)
  @products.includes(:vendor).each do |product|
    begin
      product = product.first
      sheet.add_row [product.article, product.title, product.description, product.vendor.title, product.stock, product.remote_stock]
    rescue
    end
  end
end